  "engines": {
    "node": ">=14.0"
  },
  "homepage": "https://github.com/KhalifaGad/EgyptEagles#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KhalifaGad/EgyptEagles.git"
  },
  "bugs": {
    "url": "https://github.com/KhalifaGad/EgyptEagles/issues"
  },
  "scripts": {
    "start": "NODE_ENV=Production ts-node .",
    "start:client": "cd client && npm run start",
    "start:app": "concurrently --kill-others \"npm run start\" \"npm run start:client\"",
    "dev": "NODE_ENV=Development nodemon .",
    "test": "mocha",
    "test:circular-dependencies": "madge --circular --extensions ts .",
    "lint": "eslint .",
    "lint:fix": "eslint . --ext .ts --fix",
    "seed:dummy": "NODE_ENV=Dummy ts-node ./scripts/dummy.script.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:circular-dependencies",
      "pre-push": "npm run lint && echo \"All files pass linting.\n\" || (echo \"\nSome files did not pass linting.\nUse \\`npm run lint:fix\\` to fix some of them.\nMake sure to commit these changes before pushing.\n\"; false)"
    }
  },
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "argon2": "^0.28.5",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.4.1",
    "mongoose": "^6.2.5",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "winston": "^3.6.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.10.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.4",
    "madge": "^5.0.1",
    "mocha": "^9.2.1",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2"
  }
}
